{{#if hasOrders}}
  <ul>
    {{#each orders}}
      <li>
        <h1>{{this.id}}</h1>
        <ul>
          {{#each this.products}}
            <li>{{this.title}} ({{this.quantity}})</li>
          {{/each}}
        </ul>
      </li>
    {{/each}}
  </ul>
{{else}}
  <h1>You have no orders</h1>
{{/if}}